<% if user_signed_in? %>
  <%= link_to edit_user_registration_path, :class => 'btn text-primary border-primary md:border-2 hover:bg-primary hover:text-white mx-2' do %>
  Edit Profile
  <% end %>
  <%= link_to destroy_user_session_path, :method => :delete, :class => 'btn text-primary border-primary md:border-2 hover:bg-primary hover:text-white' do %>
  Log out
  <% end %>
  <% else %>
  <%= link_to new_user_registration_path, :class => 'btn text-primary border-primary md:border-2 hover:bg-primary hover:text-white mx-2' do %>
  Sign up
  <% end %>
  <%= link_to new_user_session_path, :class => 'btn text-primary border-primary md:border-2 hover:bg-primary hover:text-white' do %>
  Log in
  <% end %>
  <% end %>
  <% if can? :access, rails_admin %>
  <%= link_to admin_dashboard_index_path, :class => 'btn text-primary border-primary md:border-2 hover:bg-primary hover:text-white mx-2' do %>
  Visit Admin Page
  <% end %>
<% end %>